{
    "name": "whitecube/bem-components",
    "description": "BEM view components in Laravel",
    "license": "MIT",
    "keywords": [
        "laravel",
        "BEM",
        "view",
        "components",
        "view components"
    ],
    "authors": [
        {
            "name": "Adrien Leloup",
            "email": "adrien@whitecube.be"
        },
        {
            "name": "Toon Van den Bos",
            "email": "toon@whitecube.be"
        }
    ],
    "require": {
        "laravel/framework": "^9.36|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Whitecube\\BemComponents\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": ["Whitecube\\BemComponents\\BemClassesServiceProvider"]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
